<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping package="org.isf.dicom.model">  <class name="FileDicomBase" table="DICOM">    <id column="DM_FILE_ID" name="idFile">      <generator class="native"/>    </id>    <property column="DM_PAT_ID" name="patId"/>    <property column="DM_FILE_NOME" name="fileName"/>    <property column="DM_FILE_ACCESSION_NUMBER" name="dicomAccessionNumber"/>    <property column="DM_FILE_INSTITUTION_NAME" name="dicomInstitutionName"/>    <property column="DM_FILE_PAT_UID" name="dicomPatientID"/>    <property column="DM_FILE_PAT_NAME" name="dicomPatientName"/>    <property column="DM_FILE_PAT_ADDR" name="dicomPatientAddress"/>    <property column="DM_FILE_PAT_AGE" name="dicomPatientAge"/>    <property column="DM_FILE_PAT_SEX" name="dicomPatientSex"/>    <property column="DM_FILE_PAT_BIRTHDATE" name="dicomPatientBirthDate"/>    <property column="DM_FILE_ST_UID" name="dicomStudyId"/>    <property column="DM_FILE_ST_DATE" name="dicomStudyDate"/>    <property column="DM_FILE_ST_DESCR" name="dicomStudyDescription"/>    <property column="DM_FILE_SER_UID" name="dicomSeriesUID"/>    <property column="DM_FILE_SER_INST_UID" name="dicomSeriesInstanceUID"/>    <property column="DM_FILE_SER_NUMBER" name="dicomSeriesNumber"/>    <property column="DM_FILE_SER_DESC_COD_SEQ" name="dicomSeriesDescriptionCodeSequence"/>    <property column="DM_FILE_SER_DATE" name="dicomSeriesDate"/>    <property column="DM_FILE_SER_DESC" name="dicomSeriesDescription"/>    <property column="DM_FILE_INST_UID" name="dicomInstanceUID"/>    <property column="DM_FILE_MODALIITY" name="modality"/>    <property column="DM_THUMBNAIL" name="dicomThumbnail"/>  </class></hibernate-mapping>